apiVersion: addons.cluster.x-k8s.io/v1beta1
kind: ClusterResourceSet
metadata:
  name: {{.Name}}-csi-resource-set
  namespace: {{.Namespace}}
spec:
  clusterSelector:
    matchLabels:
      cluster.x-k8s.io/cluster-name: {{.Name}}
  resources:
    - kind: ConfigMap
      name: {{.Name}}-oci-csi
  strategy: ApplyOnce